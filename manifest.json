{
    "background": {
        "scripts": [
            "js/jquery-2.0.0.min.js",
            "js/background.js"
        ]
    },
    "description": "我的第一個chrome插件",
    "icons": {
        "128": "/img/icon.png",
        "16": "/img/icon.png",
        "48": "/img/icon.png"
    },
    "manifest_version": 2,
    "name": "ご主人様，請問您需要什麼服務？",
    "page_action": {
        "default_icon": {
            "19": "/img/icon.png",
            "38": "/img/icon.png"
        },
        "default_popup": "popup/popup.html"
    },
    "permissions": [
        "tabs",
        "notifications",
        "https://*.google.com/*",
        "https://*.google.co.jp/*",
        "https://*.cybozu.cn/*",
        "https://d3boctest.sinaapp.com/*"
    ],
    "version": "0.0.1"
}