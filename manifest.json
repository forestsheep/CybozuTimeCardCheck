{
    "background": {
        "scripts": [
            "js/jquery-2.0.0.min.js",
            "js/background.js",
            "js/jquery.cookie.js"
        ]
    },
    "description": "每天早上不再捉鸡",
    "icons": {
        "128": "/icons/title128.png",
        "16": "/icons/title16.png",
        "48": "/icons/title64.png"
